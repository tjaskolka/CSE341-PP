{
    "env": {
        "es2021": true,
        "browser": true,
        "node": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
        {
            "endOfLine": "auto"
        }
    ]
    }
}